<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<HBox spacing="8.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.ninjaphase.pokered.editor.dialog.database.DatabaseGeneralController">
   <children>
      <TitledPane animated="false" collapsible="false" prefHeight="514.0" prefWidth="460.0" text="General Settings">
         <content>
            <VBox prefHeight="200.0" prefWidth="100.0" spacing="8.0">
               <children>
                  <GridPane prefHeight="179.0" prefWidth="434.0">
                     <children>
                        <TextField fx:id="txtName" promptText="Enter the Story Name..." GridPane.columnIndex="1" />
                        <Label text="Story Name:" />
                        <Label text="Story Description:" GridPane.rowIndex="1" />
                        <TextArea fx:id="txtDescription" promptText="Enter a description..." GridPane.columnSpan="2" GridPane.rowIndex="2" />
                     </children>
                     <columnConstraints>
                        <ColumnConstraints hgrow="SOMETIMES" maxWidth="212.0" minWidth="10.0" prefWidth="114.0" />
                        <ColumnConstraints hgrow="SOMETIMES" maxWidth="360.0" minWidth="10.0" prefWidth="320.0" />
                     </columnConstraints>
                     <rowConstraints>
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                        <RowConstraints maxHeight="55.0" minHeight="10.0" prefHeight="27.0" vgrow="SOMETIMES" />
                        <RowConstraints maxHeight="103.0" minHeight="10.0" prefHeight="92.0" vgrow="SOMETIMES" />
                     </rowConstraints>
                  </GridPane>
               </children>
            </VBox>
         </content>
      </TitledPane>
      <TitledPane animated="false" collapsible="false" prefHeight="514.0" prefWidth="225.0" text="Debug Settings">
         <content>
            <VBox spacing="8.0">
               <children>
                  <Label text="Initial Party:" />
                  <ListView fixedCellSize="6.0" prefHeight="134.0" prefWidth="200.0" />
                  <HBox>
                     <children>
                        <Button mnemonicParsing="false" prefWidth="80.0" text="Edit" />
                        <Pane HBox.hgrow="ALWAYS" />
                        <Button mnemonicParsing="false" prefWidth="80.0" text="Delete" />
                     </children>
                  </HBox>
               </children>
            </VBox>
         </content>
      </TitledPane>
   </children>
   <padding>
      <Insets bottom="8.0" left="8.0" right="8.0" top="8.0" />
   </padding>
</HBox>
